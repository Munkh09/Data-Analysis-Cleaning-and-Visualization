#then we insert the data into the DAT file manually separating with comma
echo passenger2.DAT

#once the passenger.DAT file is filled out with 2 columns of numbers separated with comma, we can run the code
##here are the gnuplot commands
#!/usr/local/bin/gnuplot -persist
set style fill transparent solid 0.2 noborder

set datafile separator ','

set out 'passenger2.png'

#set dummy u, v
#set key fixed right top vertical Right noreverse enhanced #autotitle box lt black linewidth 1.000 dashtype solid
#set parametric
set title "Trip Distance Total Amount Average Correlation"
set xlabel "Trip Distance"
#set colorbox vertical origin screen 0.9, 0.2 size screen 0.05, 0.6 front  noinvert bdefault
NO_ANIMATION = 1
set autoscale x
set autoscale y
## Last datafile plotted: "hemisphr.dat"
plot 'passenger2.dat' using 1:2 with circles lc rgb 'red'
